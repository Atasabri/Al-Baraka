@model IEnumerable<Branch>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
    // StaticDataValues staticData = ViewBag.StaticData as StaticDataValues;
    var currentCulture = ViewContext.HttpContext.Features.Get<IRequestCultureFeature>().RequestCulture.UICulture;
}
@inject IViewLocalizer Localizer
@inject IStaticDataValues staticData
@section Banner{
    <!-- banner -->
    <div class="banner-silder">
        <div id="JiSlider" class="jislider">
            <ul>
                <li>
                    <div class="w3layouts-banner-top" style="background: url(/Uploads/StaticImages/@staticData.GetValue("Home_Banner1",null))  no-repeat 0px 0px;background-size:cover;">

                        <div class="container">
                            <div class="agileits-banner-info">
                                <span>@Localizer["100 Branch"]</span>
                                <h3>@Localizer["Branches All Over The RePublic"]</h3>
                                <p>@Localizer["You Can Find Our Laboratories in All Parts of The RePublic and All The Governorates of Egypt to Facilitate You Always Seeking us."]</p>

                            </div>
                        </div>
                    </div>
                </li>
                <li>
                    <div class="w3layouts-banner-top w3layouts-banner-top1" style="background: url(/Uploads/StaticImages/@staticData.GetValue("Home_Banner2",null))  no-repeat 0px 0px;background-size:cover;">
                        <div class="container">
                            <div class="agileits-banner-info">
                                <span>@Localizer["Our Services"]</span>
                                <h3>@Localizer["Services In The Field of Medical Analysis"]</h3>
                                <p>@Localizer["Our Laboratories Provide Services at The Highest Level in The Field of Medical Analysis by Dealing with Equipped Devices at The Highest Level."]</p>

                            </div>
                        </div>
                    </div>
                </li>
                <li>
                    <div class="w3layouts-banner-top w3layouts-banner-top2" style="background: url(/Uploads/StaticImages/@staticData.GetValue("Home_Banner3",null))  no-repeat 0px 0px;background-size:cover;">
                        <div class="container">
                            <div class="agileits-banner-info">
                                @*<span>@Localizer["The Quality"]</span>
                                <h3>@Localizer["The Highest Level Coefficient"]</h3>
                                <p>@Localizer["Al-Baraka Laboratories Are Equipped With The Highest Level andTthe International Quality Standards, and They Guarantee You Safety in Dealing, Quality and Professionalism in Work."]</p>*@

                            </div>

                        </div>
                    </div>
                </li>

            </ul>
        </div>
    </div>

    <!-- //banner -->

}

<!-- about -->
<div class="about" id="about">
    <div class="container">
        <h2 class="w3_heade_tittle_agile">@Localizer["Welcome to Al-Baraka Labs"]</h2>
        <br />
        <p class="ab">@staticData.GetValue("Home_Welcome", currentCulture.Name)</p>

        <div class="about-w3lsrow">

            <div class="col-md-6 w3about-img">
                <img src="~/Uploads/StaticImages/@(staticData.GetValue("Home_Image1",null))" alt=" " class="img-responsive">
            </div>
            <div class="col-md-6 col-sm-7 w3about-img two">
                <p class="w3about-text">
                    <h5 class="w3l-subtitle">@staticData.GetValue("Home_Head1", currentCulture.Name)</h5>
                    <p>
                        @staticData.GetValue("Home_Description", currentCulture.Name)</p>
                        <div class="read"><a asp-action="About" class="hvr-rectangle-in">@Localizer["Read More"]</a></div>
                    </p>
            </div>
            <div class="clearfix"> </div>
        </div>

    </div>
</div>
<!-- /about-bottom -->
